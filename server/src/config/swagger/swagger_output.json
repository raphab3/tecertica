          "example": "raphab32"
        },
        "password": {
          "type": "string",
          "example": "123456789"
        }
      },
      "required": ["Managername", "password"],
      "xml": {
        "name": "AddManager"
      }
    },
    "AddManager": {
      "type": "object",
      "properties": {
        "Managername": {
          "type": "string",
          "example": "maz√©2021"
        },
        "password": {
          "type": "string",
          "example": "Manager4231"
        },
        "email": {
          "type": "string",
          "example": "email@email.com"
        }
      },
      "required": ["name"],
      "xml": {
        "name": "AddManager"
      }
    },
    "UpdateManager": {
      "type": "object",
      "properties": {
        "full_name": {
          "type": "string",
          "example": "Marcos Paulo"
        },
        "Managername": {
          "type": "string",
          "example": "marquin2001"
        },
        "password_blocked": {
          "type": "boolean",
          "example": false
        },
        "date_expiration": {
          "type": "string",
          "example": "2000-01-01"
        },
        "counter_attemps": {
          "type": "number",
          "example": 2
        },
        "password": {
          "type": "string",
          "example": "senha123"
        },
        "disabled": {
          "type": "boolean",
          "example": false
        },
        "profile_image": {
          "type": "string",
          "example": "image.jpg"
        },
        "first_access": {
          "type": "boolean",
          "example": false
        },
        "business_divisions": {
          "type": "array",
          "items": {
            "type": "object"
          },
          "example": [
            {
              "id": "abcd-1234-ef56-78gh"
            }
          ]
        },
        "groups_permissions": {
          "type": "array",
          "items": {
            "type": "object"
          },
          "example": [
            {
              "id": ""
            }
          ]
        },
        "email": {
          "type": "string",
          "example": "email@email.com"
        },
        "phone": {
          "type": "string",
          "example": "43 6774-0958"
        },
        "office": {
          "type": "string",
          "example": "Office"
        },
        "business": {
          "type": "string",
          "example": "businessID"
        }
