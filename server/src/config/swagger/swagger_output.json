            }
          }
        ],
        "responses": {
          "201": {
            "description": "Managers created successfully."
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/Managers": {
      "post": {
        "tags": ["Managers"],
        "description": "Endpoint to add a Manager.",
        "produces": ["application/xml"],
        "parameters": [
          {
            "name": "obj",
            "in": "body",
            "description": "Managers information.",
            "required": true,
            "schema": {
              "$ref": "#/definitions/AddManager"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Managers created successfully."
          },
          "500": {
            "description": "Internal Server Error"
          }
